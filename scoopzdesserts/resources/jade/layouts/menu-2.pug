extends ../master

block title
    title Bakery | Menu 2

block page_header

    - var currentPage = 'page'
    include ../utils/mixins
    include ../common/common-element
    include ../partials/modules/search
    include ../partials/header-2

block page_content
    .ps-section--hero
        img(src="images/hero/03.jpg" alt="")
        .ps-section__content.text-center
            h3.ps-section__title Menu
            .ps-breadcrumb
                ol.breadcrumb
                    li: a(href="index.html") Home
                    li.active Menu
    - var menuType = 2;
    include ../partials/section/menu-listing
    - aboutType = 2;
    include ../partials/section/about-intro
    - var menuType = 1;
    include ../partials/section/menu-listing
    include ../partials/section/contact
    include ../partials/section/subscribe

    include ../partials/footer
    include ../partials/modules/product-quickview








