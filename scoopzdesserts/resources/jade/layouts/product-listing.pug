extends ../master

block title
    title Bakery | Product List
block page_header

    - var currentPage = 'product'
    include ../utils/mixins
    include ../common/common-element
    include ../partials/modules/search
    include ../partials/header-2

block page_content

    // Heros
    .ps-section--hero
        img(src="images/hero/01.jpg" alt="")
        .ps-section__content.text-center
            h3.ps-section__title OUR BAKERY
            .ps-breadcrumb
                ol.breadcrumb
                    li: a(href="index.html") Home
                    li.active Shop
    - var shopType = "listing";
    include ../partials/modules/shop
    include ../partials/section/subscribe

    //footer
    include ../partials/footer









