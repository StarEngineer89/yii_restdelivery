block module
    .ps-shop
        each val in product_listing
            .ps-product--list.ps-product--list-large
                .ps-product__thumbnail
                    a(href='product-detail.html').ps-product__overlay
                    img(src=val.imagepath alt="")
                .ps-product__content
                    h4.ps-product__title
                        a(href='product-detail.html') #{val.title}
                    select.ps-rating
                        option(value='1') 1
                        option(value='1') 2
                        option(value='1') 3
                        option(value='1') 4
                        option(value='5') 5
                    p.ps-product__price
                        del #{val.originPrice}
                        | #{val.salePrice}
                    p #{val.description}
                    footer.ps-product__footer.clearfix
                        a(href='cart.html').ps-btn.ps-btn--sm Order now
                            i.fa.fa-angle-right
                        ul.ps-product__action
                            li: a(href='#' data-tooltip="Add to wishlist"): i.ps-icon--heart
                            li: a(  href='#' data-tooltip="Compare"): i.ps-icon--reload
    +pagination