block section
    include ../../data/partner
    .ps-section.ps-section--partner
        .container
            .owl-slider(
                data-owl-auto= 'true',
                data-owl-loop= 'true',
                data-owl-speed= '10000',
                data-owl-gap= '40',
                data-owl-nav= 'false',
                data-owl-dots= 'false',
                data-owl-animate-in= '',
                data-owl-animate-out= '',
                data-owl-item= '6',
                data-owl-item-xs= '3',
                data-owl-item-sm= '4',
                data-owl-item-md= '5',
                data-owl-item-lg= '6',
                data-owl-nav-left = '<i class="fa fa-angle-left"></i>',
                data-owl-nav-right = '<i class="fa fa-angle-right"></i>',
            )
                - for (var i = 1; i < 9; i ++)
                    a(href='#')
                        img(src="images/partner/"+i+".png" alt="")